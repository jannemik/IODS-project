#Define a new logical column 'high_use'
alc <- mutate(alc, high_use = alc_use > 2)
#Explore the data with glimpse
glimpse(alc)
#Explore the data with glimpse
glimpse(alc)
#Save the joined and modified data set
write.csv(alc, file = "alc.csv", row.names = FALSE)
